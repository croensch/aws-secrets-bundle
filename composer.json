{
    "name": "constup/aws-secrets-bundle",
    "type": "symfony-bundle",
    "description": "Use AWS Secrets as service container parameters in Symfony.",
    "keywords": ["Symfony", "AWS", "ENV", "Secrets Manager"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Mizzi",
            "email": "joe@casechek.com"
        },
        {
            "name": "James Matsumura",
            "email": "james@casechek.com"
        },
        {
            "name": "Steven Lehn",
            "email": "steven.lehn@casechek.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/dependency-injection": "^5.3",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/framework-bundle": "^5.3",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3 | ^4.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "aws/aws-sdk-php": "^3.191"
    },
    "autoload": {
        "psr-4": {
            "Constup\\AwsSecretsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Constup\\AwsSecretsBundle\\Tests\\": "tests/"
        }
    }
}
